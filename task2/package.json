{
  "name": "stock-trades-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node -r esm bin/www",
    "dev": "nodemon -r esm bin/www",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate:new": "sequelize migration:generate --name",
    "db:seed": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo",
    "db:seed:new": "sequelize seed:generate --name",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "prestart": "npm install",
    "pretest": "npm install",
    "test": "cross-env PORT=8001 ./node_modules/.bin/mocha --require tsx --reporter mocha-multi-reporters --reporter-options configFile=config.json --exit"
  },
  "devDependencies": {
    "bluebird": "^3.7.2",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "mocha": "^10.4.0",
    "mocha-junit-reporter": "^2.2.1",
    "mocha-multi-reporters": "^1.5.1",
    "nodemon": "^3.1.2",
    "prettier": "^3.2.5",
    "tsx": "^4.11.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "esm": "^3.2.25",
    "express": "^4.19.2",
    "express-http-context": "^1.2.4",
    "log4js": "^6.9.1",
    "sequelize": "^6.37.3",
    "sqlite3": "^5.1.7"
  }
}
